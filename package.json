        "start": "pm2 start ecosystem.config.js",
        "dev": "npm-run-all --parallel dev:*",
        "prod": "npm-run-all --parallel prod:*",
        "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
        "dev:build-server": "webpack --progress --profile --colors --config webpack/webpack.server.config.js --watch",
        "dev:build-client": "webpack --progress --profile --colors --config webpack/webpack.client.config.js --watch",
        "prod:server": "node build/bundle.js",
        "prod:build-server": "NODE_ENV=production webpack --colors --config webpack/webpack.server.config.js",
        "prod:build-client": "NODE_ENV=production webpack --colors --config webpack/webpack.client.config.js"
    },
